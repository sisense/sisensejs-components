{
  "name": "@sisense/sjs",
  "version": "0.0.9",
  "description": "A component-based way to embed SisenseJS into your application.",
  "author": "Sisense Inc. <github@sisense.com> (https://www.sisense.com/)",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/sjs/sjs.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --watch",
    "dev.react": "yarn run build.watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll",
    "coverage": "stencil test --spec --coverage",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.18.0",
    "@stencil/react-output-target": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "prettier": "^2.7.1",
    "puppeteer": "^10.0.0",
    "typescript": "^4.8.3"
  }
}
